<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:cusControl="clr-namespace:BitEd.CustomControl">
    <cusControl:DropDownButton x:Key="SpriteEntityPropertyTemplate">
        <cusControl:DropDownButton.Resources>
            <ContextMenu x:Key="ContextMenuItems">
                <Menu ItemsSource="{Binding ProjectVM.ProjectAssets, Source={StaticResource Locator}}">
                    <Menu.Resources>
                        <Style TargetType="{x:Type MenuItem}">
                            <!--<Setter Property="Command" Value="{Binding Command}" />-->
                            <Setter Property="Header" Value="{Binding Name}" />
                            <!--<Setter Property="Icon">
                                <Setter.Value>
                                    <Image Source="{Binding MenuIcon}" Height="16px" Width="16px" />
                                </Setter.Value>
                            </Setter>-->
                            <Setter Property="ItemsSource" Value="{Binding Childs}"/>
                        </Style>
                    </Menu.Resources>
                </Menu>
            </ContextMenu>
        </cusControl:DropDownButton.Resources>
    </cusControl:DropDownButton>
</ResourceDictionary>